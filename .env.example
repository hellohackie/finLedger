# Backend Configuration
# Change VITE_BACKEND_TYPE to switch between backends
# Options: 'nodejs' or 'java'
VITE_BACKEND_TYPE=nodejs

# For Java backend, you can also set custom URL
# VITE_JAVA_BACKEND_URL=http://localhost:8080

# For Node.js backend, you can also set custom URL  
# VITE_NODEJS_BACKEND_URL=http://localhost:5000

# Database Configuration (for both backends)
DATABASE_URL=your_postgresql_connection_string

# Authentication (for both backends)
REPL_ID=your_replit_app_id
SESSION_SECRET=your_session_secret
REPLIT_DOMAINS=your_domain

# Java-specific configuration
ISSUER_URL=https://replit.com/oidc
CLIENT_SECRET=optional_oauth_client_secret